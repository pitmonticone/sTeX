    \StrCount\filename@oldpath\@Dot[\filename@lastdot]%
    \ifnum\filename@lastdot>0%
        \StrBefore[\filename@lastdot]\filename@oldpath%
          \@Dot[\filename@oldpath]%
        \edef#2{\filename@oldpath}%
    \else%
        \edef#2{\filename@oldpath}%
